	pages = {58-67},
	researcherid-numbers = {Hyder, Ayaz/A-3738-2016},
	title = {PM$_{2.5}$ Exposure and Birth Outcomes Use of Satellite- and Monitor-Based Data},
	unique-id = {ISI:000328205200012},
