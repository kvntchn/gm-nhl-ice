	doi = {10.1002/mds.20513},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/mds.20513},
	journal = {Movement Disorders},
	keywords = {idiopathic Parkinson's disease (PD), phenotypic description, disease progression, etiology, populationâbased study},
	number = {9},
	pages = {1133-1142},
	title = {Clinical characteristics in early Parkinson's disease in a central California populationâbased study},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/mds.20513},
	volume = {20},
	year = {2005},
	Bdsk-Url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1002/mds.20513},
